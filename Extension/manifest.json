{
  "manifest_version": 2,
  "name": "RT-NIDS",
  "description": "Test",
  "permissions": ["activeTab", "declarativeContent", "storage","tabs","https://ajax.googleapis.com/","<all_urls>"],
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}

